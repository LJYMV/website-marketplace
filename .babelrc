{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "ie": "10"
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "helpers": false
      }
    ],
    [
      "module-resolver",
      {
        "alias": {
          "~": "./src",
          "~components": "./src/@/components/src",
          "~ui": "./src/ui"
        }
      }
    ],
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-jsx"
  ]
}
